{
  "name": "dishi",
  "private": true,
  "version": "2.1.7",
  "description": "2.1.0, custom mac dmg",
  "main": "index.js",
  "module": "src/index.js",
  "sideEffets": false,
  "files": [
    "src"
  ],
  "scripts": {
    "build": "rollup -c"
  },
  "dependencies": {
    "qiao-electron": "^3.4.5",
    "qiao-json": "^3.1.2",
    "qiao-sqlite": "^3.2.3",
    "qiao-x-menu": "^3.4.5",
    "qiao-x-window": "^3.4.3"
  },
  "nx": {
    "namedInputs": {
      "default": [
        "{projectRoot}/src/**/*",
        "{projectRoot}/package_bak.json"
      ]
    },
    "targets": {
      "build": {
        "inputs": [
          "default"
        ],
        "outputs": [
          "{workspaceRoot}/packages/dishi-electron/dist/**/*"
        ]
      }
    }
  }
}
